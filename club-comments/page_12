	function delComment(commentid,clubid)
	{
		var confirmVal = confirm('Remove this comment?');

		if (confirmVal)
			{
			var comObj = document.getElementById("comment"+commentid);
			comObj.innerHTML = '<img src="http://cdn.myanimelist.net/images/xmlhttp-loader.gif" />';
			$.get("/includes/ajax.inc.php?t=10&comid="+commentid+"&cid="+clubid, function(data)
